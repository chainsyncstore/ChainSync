  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "// Testing Scripts": "",
    "test": "npm run test:server && npm run test:utils",
    "test:server": "set NODE_OPTIONS=--experimental-vm-modules --experimental-specifier-resolution=node && jest",
    "test:utils": "jest tests/utils --config=./jest.config.ts --collectCoverage=true",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test && npm run test:e2e -- --reporter=github",
    "// Security Scripts": "",
    "security:audit": "npm audit --omit=dev",
    "security:audit:all": "npm audit",
    "security:audit:json": "npm audit --json > security-audit.json",
    "security:fix": "npm audit fix --save",
    "security:deps": "node scripts/check-dependency-health.js",
    "security:check": "npm run security:audit && npm run security:deps",
    "// Development Scripts": "",
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "// Build Scripts": "",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npx vite build",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build:staging": "cross-env NODE_ENV=staging npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "// Runtime Scripts": "",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/server/index.js",
    "start:production": "cross-env NODE_ENV=production node dist/server/index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "// Database Scripts": "",
    "db:push": "drizzle-kit push --force --config=./drizzle.config.ts",
    "db:seed": "tsx db/seed.ts",
    "db:backup": "node scripts/backup.js",
    "db:backup:upload": "node scripts/backup.js --upload",
    "db:restore": "node scripts/restore.js",
    "db:restore:latest": "node scripts/restore.js --latest",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "// Linting and Type Checking": "",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:unused-vars": "eslint . --ext .ts,.tsx --rule 'no-unused-vars: error' --quiet",
    "lint:react-hooks": "eslint ./client/src --ext .tsx --rule 'react-hooks/rules-of-hooks: error' --rule 'react-hooks/exhaustive-deps: error' --quiet",
    "lint:imports": "eslint . --ext .ts,.tsx --rule 'import/no-duplicates: error' --rule 'import/order: error' --quiet",
    "lint:fix-all": "npm run lint && npm run lint:unused-vars -- --fix && npm run lint:react-hooks -- --fix && npm run lint:imports -- --fix",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "quality-check": "npm run lint:fix-all && npm run type-check && node scripts/sql-security-check.js && node scripts/check-drizzle-patterns.js",
    "// Health and Monitoring": "",
    "health": "node scripts/health-check.js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "audit:fix": "npm audit fix --production",
    "// Deployment": "",
    "prepare:deploy": "npm run build:production && npm run check",
    "postinstall": "npm run build",
    "// Git Hooks": "",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
