{
  "name": "chainsync",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"ts-node-dev --respawn --transpile-only src/index.ts\"",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json --outDir dist/server && node scripts/create-server-package.js",
    "start": "node dist/server/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:security": "jest --testPathPattern=security",
    "test:performance": "jest --testPathPattern=performance",
    "test:contract": "jest --testPathPattern=contract",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:visual": "jest --testPathPattern=visual",
    "test:load": "artillery run test/load/load-test.yml",
    "test:stress": "artillery run test/load/stress-test.yml",
    "test:penetration": "npm run test:security",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "db:introspect": "drizzle-kit introspect:pg",
    "db:seed": "tsx db/seed.ts",
    "quality:check": "npm run lint && npm run type-check && npm run test:coverage",
    "quality:gates": "npm run quality:check && npm run test:security && npm run test:performance",
    "docs:generate": "swagger-jsdoc -d swaggerDef.js -o docs/swagger.json",
    "docs:serve": "swagger-ui-express docs/swagger.json",
    "deploy:check": "node scripts/production-readiness-check.js",
    "deploy:blue-green": "node scripts/deploy-blue-green.js",
    "deploy:validate": "ts-node deploy/validate-deployment.ts",
    "monitor:start": "node scripts/production-monitor.js",
    "backup:create": "ts-node deploy/backup/backup-recovery.ts",
    "security:audit": "npm audit --audit-level=high",
    "rebuild": "node scripts/rebuild-and-deploy.js",
    "update:deps": "node scripts/update-dependencies.js"
  },
